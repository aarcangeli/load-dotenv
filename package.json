{
  "name": "typescript-action",
  "version": "0.0.0",
  "private": true,
  "description": "TypeScript template action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsc && line-endings -l LF -w lib/**",
    "format": "prettier --write src/**/*.ts",
    "format-check": "prettier --check src/**/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "generate-docs": "node lib/misc/generate-docs.js",
    "all": "npm run build && npm run format && npm run lint && npm run package && npm run package",
    "pre-commit": "npm run format && npm run build && npm run generate-docs && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/actions/typescript-action.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.8.0",
    "@folkforms/line-endings": "^0.0.12"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.27.1",
    "@vercel/ncc": "^0.31.1",
    "dotenv": "^16.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-github": "^4.3.2",
    "js-yaml": "^4.1.0",
    "prettier": "2.6.2",
    "typescript": "^4.6.4"
  }
}
